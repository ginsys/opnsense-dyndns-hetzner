# Copy this file to secret.yaml and fill in your actual values
# DO NOT commit secret.yaml to version control
#
# Apply with: kubectl apply -f secret.yaml -n opnsense-dyndns-hetzner
apiVersion: v1
kind: Secret
metadata:
  name: opnsense-dyndns-hetzner-secrets
type: Opaque
stringData:
  OPNSENSE_API_KEY: "<your-opnsense-api-key>"
  OPNSENSE_API_SECRET: "<your-opnsense-api-secret>"
  HETZNER_DNS_TOKEN: "<your-hetzner-dns-api-token>"
